<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hhhhentai.ulife">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA"/>

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/ulife_title"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".MainActivity"
            android:theme="@android:style/Theme.Translucent">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".main_part" />
        <activity
            android:name=".Register_phonenum"
            android:theme="@android:style/Theme.Translucent" />
        <activity
            android:name=".Forget"
            android:theme="@android:style/Theme.Translucent" />
        <activity android:name=".newsActivity" />
        <activity
            android:name=".Register_userinfo"
            android:theme="@android:style/Theme.Translucent" />
        <activity android:name=".History" />
        <activity android:name=".Seting" />
        <activity android:name=".Set_userInfo" />
        <activity
            android:name="com.example.hhhhentai.UseTool.WeatherActivity"
            android:theme="@android:style/Theme.Translucent" />
        <activity android:name=".ChangeHeadphoto" />
        <activity android:name="com.example.hhhhentai.UseTool.ExpressageActivity" />
        <activity android:name="com.example.hhhhentai.UseTool.SchoolDataActivity" />
        <activity android:name=".PictureActivity">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".publishActivity"></activity>
    </application>

</manifest>